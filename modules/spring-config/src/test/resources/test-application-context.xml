<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:component-scan base-package="language"/>

    <bean id="bfpp" class="language.LanguageParserTestCase$CustomBeanFactoryPostProcessor"/>

    <bean id="apple" class="org.mule.tck.testmodels.fruit.Apple"/>
    
    <bean id="banana" class="org.mule.tck.testmodels.fruit.Banana"/>
    
    <bean id="orange" class="org.mule.tck.testmodels.fruit.Orange"/>
    
    <bean id="kiwi" class="org.mule.tck.testmodels.fruit.Kiwi"/>
</beans>
